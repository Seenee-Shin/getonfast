<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">

<properties>

	<!-- 공간 타입 조회 -->
	<entry key="selectSpaceType">
		SELECT * FROM SPACE_TYPE
		WHERE SPACE_TYPE_ST = 1
	</entry>
	
	<entry key="selectExhibit">
		SELECT * FROM (
			SELECT ROWNUM RNUM, EX_NO, EX_NM, EX_SUB_NM, EX_IMG FROM EXHIBIT
		)
		WHERE RNUM <![CDATA[ <= ]]> 4
	</entry>
	
</properties>